{
    "env": {
        "browser": false,
        "commonjs": false,
        "es6": true,
        "node": true
    },
    "parserOptions": {
        "ecmaVersion":2017,
        "ecmaFeatures": {
            "jsx": true
        },
        "sourceType": "module"
    },
    "extends": [
        "eslint:recommended",
        "eslint-config-airbnb-base",
        "plugin:import/errors",
        "plugin:import/warnings",
        "prettier",
        "plugin:wdio/recommended"
    ],
    "plugins": ["import", "prettier", "wdio"],
    "rules": {
        "prettier/prettier": [
            "warn",
            {
                "singleQuote": true,
                "printWidth": 120
            }
        ],
        "no-const-assign": "warn",
        "no-this-before-super": "warn",
        "no-undef": "warn",
        "no-unreachable": "warn",
        "no-unused-vars": "warn",
        "constructor-super": "warn",
        "valid-typeof": "warn",
        "linebreak-style": [ "error", "unix" ],
        "no-console": [ 0, "error" ],
        "indent": [ "error", 2 ],
        "semi": [ "error", "always" ],
        "quotes":["error", "single"]
    },
    "globals": {
        "Given": true,
        "When": true,
        "Then": true,
        "And": true,
        "But": true,
        "helpers":true,
        "browser": false,
        "log": true,
        "date": false,
        "dateTime": true,
        "startDateTime": false,
        "endDateTime": false,
        "browserName": false,
        "BROWSER_NAME": false,
        "reportName": true,
        "projectName": true,
        "projectReportName": true,
        "settings": false,
        "envConfig": true,
        "request": true,
        "assert": true,
        "expect": true,
        "shared": true,
        "DELAY_100ms": true,
        "DELAY_200ms": true,
        "DELAY_300ms": true,
        "DELAY_500ms": true,
        "DELAY_1s": true,
        "DELAY_2s": true,
        "DELAY_3s": true,
        "DELAY_5s": true,
        "DELAY_10s": true,
        "DELAY_15s": true,
        "DELAY_20s": true,
        "noImplicitAny": true,
        "noImplicitThis": true,
        "strictNullChecks": true,
        "strictFunctionTypes": false,
        "noEmit": true,
        "forceConsistentCasingInFileNames": true
    }
}