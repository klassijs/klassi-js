{
  "name": "klassi-js",
  "version": "3.0.0",
  "description": "A debuggable Javascript BDD testing framework. It's simple and easy to use, its built on webdriverio and nodejs",
  "Creator": {
    "name": "Larry Goddard",
    "email": "larryg@klassitech.co.uk",
    "twitter": "twitter.com/lcgoddard",
    "url": "linkedin.com/in/larryg"
  },
  "license": "Apache-2.0",
  "keywords": [
    "api",
    "api testing",
    "bdd",
    "browserstack",
    "cucumber-js",
    "javascript",
    "framework",
    "functional testing",
    "tdd",
    "testing",
    "testing-framework",
    "webdriver",
    "webdriverio"
  ],
  "repository": {
    "type": "git",
    "url": "https://github.com/larryg01/klassi-js"
  },
  "main": "index.js",
  "scripts": {
    "test": "export PROJECT_NAME=example-test-suite&& cd projects/example-test-suite&& node ../../index.js -d -t @api",
    "all": "export PROJECT_NAME=example-test-suite&& cd projects/example-test-suite&& node ../../index.js -e -d",
    "dev": "export PROJECT_NAME=example-test-suite&& cd projects/example-test-suite&& node ../../index.js -d -t",
    "bslocal": "export PROJECT_NAME=example-test-suite&& cd projects/example-test-suite&& node ../../index.js -e-d-w browserstack -x",
    "cibsuat": "export PROJECT_NAME=example-test-suite&& cd projects/example-test-suite&& node ../../index.js -e -d -t @search -w browserstack -x",
    "cibsdev": "export PROJECT_NAME=example-test-suite&& cd projects/example-test-suite&& node ../../index.js -d -t @s3 -w browserstack -x",
    "pkgcheck": "yarn install --check-files"
  },
  "homepage": "https://github.com/larryg01/klassi-js#readme",
  "dependencies": {
    "aws-sdk": "^2.740.0",
    "axe-core": "^4.0.1",
    "browserstack-local": "^1.4.5",
    "chai": "^4.2.0",
    "chalk": "^4.1.0",
    "commander": "^6.0.0",
    "cross-env": "^7.0.2",
    "cucumber": "^6.0.5",
    "cucumber-html-reporter": "^5.0.0",
    "cucumber-pretty": "^6.0.0",
    "fs-extra": "^9.0.1",
    "got": "^9.6.0",
    "merge": "^1.2.1",
    "node-dir": "^0.1.17",
    "node-resemble-js": "^0.2.0",
    "nodemailer": "^6.3.0",
    "recursive-readdir": "^2.2.2",
    "require-dir": "^1.2.0",
    "webdriverio": "^6.4.2",
    "winston": "^3.2.1"
  },
  "devDependencies": {
    "chromedriver": "^85.0.0",
    "devtools": "^6.4.1",
    "eslint": "^7.6.0",
    "eslint-config-airbnb-base": "^14.2.0",
    "eslint-config-prettier": "^6.11.0",
    "eslint-plugin-import": "^2.3.0",
    "eslint-plugin-prettier": "^3.1.1",
    "eslint-plugin-wdio": "^6.0.12",
    "geckodriver": "^1.20.0",
    "prettier": "^2.1.1"
  }
}
