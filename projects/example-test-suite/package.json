{
  "name": "klassi-js",
  "version": "3.0.0",
  "description": "A debuggable JS BDD framework that uses nodeJs, webdriverio, JavaScript and cucumber",
  "Author": {
    "name": "Larry Goddard",
    "email": "larryg@klassitech.co.uk",
    "twitter": "twitter.com/Larry_G0",
