{
  "name": "project-name",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/{repoName}"
  },
  "main": "index.js",
  "scripts": {
    "all": "cross-env PROJECT_NAME={projectName}&& node ../../index.js -ed",
    "dev": "cross-env PROJECT_NAME={projectName}&& node ../../index.js -dt @tagName",
    "bslocal": "cross-env PROJECT_NAME={projectName}&& node ../../index.js -edw browserstack -x",
    "cibsdev": "cross-env PROJECT_NAME={projectName}&& node ../../index.js -edt @tagName -w browserstack -x",
    "pkgchecks": "yarn check"
  }

}
