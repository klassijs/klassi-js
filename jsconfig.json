{
  "compilerOptions": {
    "module": "commonjs",
    "lib": [
      "es6",
      "dom"
    ],
    "noImplicitAny": true,
    "noImplicitThis": true,
    "strictNullChecks": true,
    "strictFunctionTypes": false,
    "baseUrl": "./",
    "noEmit": true,
    "forceConsistentCasingInFileNames": true
  },
  "exclude": [
    "./node_modules",
  "log",
  "features",
  "cssImages",
  "reports",
  "artifact"
  ],
  "files": [
    "./globals.d.ts"
  ]
}